
<section class="dailyUi">
<% for (let i=0; i<ddbb.dailyDB.length; i++) { %> 
     
     <!--DailyCards-->
   
<div class="boxCard">
     <a href="/dailies/<%=(i+1)%>" class="boxLink">
       <div class="contenedorImagen">

           <img src=<%=`${ddbb.dailyDB[i].imagen}`%> alt="">

       </div>
       <div class="infoCaja">
           <h3>Daily UI <%= ddbb.dailyDB[i].numeroDia %></h3>
           <p><%= ddbb.dailyDB[i].title %></p>
       </div>
     </a>
   </div>
<% }%> 
</section>